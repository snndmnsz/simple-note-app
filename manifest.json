{
  "manifest_version": 2,
  "name": "Simple Note",
  "version": "1.0.0",
  "description": "A simple minimalist note app for browser.",
  "icons": {
    "16": "images/icon48.png",
    "48": "images/icon48.png",
    "128": "images/icon48.png"
  },
  "author": "snndmnsz",
  "incognito": "not_allowed",
  "short_name": "SN",
  "browser_action": {
    "deafult_icon": "images/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Simple Note"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/main.js"]
    }
  ]
}
