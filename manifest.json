{
    "manifest_version":2,
    "name":"Simple Note",
    "version":"1.0.0",
    "description":"A simple minimalist note app for browser.",
    "icons":{
        "48":"images/icon48.png"
    },
    "browser_action":{
        "deafult_icon":"images/icon48.png",
        "default_popup":"popup.html"
    },
    "content_scripts":[{
      "matches": ["<all_urls>"],
      "js":["js/main.js"]
    }
    ]
}
